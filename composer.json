{
    "name": "symfony/assetic-bundle",
    "description": "Integrates Assetic into Symfony2",
    "keywords": ["assets", "compression", "minification"],
    "homepage": "https://github.com/sanpii/AsseticBundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Kris Wallsmith",
            "email": "kris.wallsmith@gmail.com",
            "homepage": "http://kriswallsmith.net/"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "symfony/asset": "*",
        "symfony/framework-bundle": "*",
        "symfony/dependency-injection": "*",
        "symfony/console": "*",
        "symfony/http-kernel": "*",
        "symfony/templating": "*",
        "symfony/yaml": "*",
        "sanpi/assetic": "*"
    },
    "require-dev": {
        "patchwork/jsqueeze": "*",
        "kriswallsmith/spork": "*",
        "phpunit/phpunit": "~*",
        "symfony/twig-bundle": "*",
        "symfony/dom-crawler": "*",
        "symfony/css-selector": "*",
        "symfony/phpunit-bridge": "*"
    },
    "replace": {
        "symfony/assetic-bundle": "self.version"
    },
    "conflict": {
        "twig/twig": "<1.27",
        "kriswallsmith/spork": "<=0.2"
    },
    "suggest": {
        "symfony/twig-bundle": "to use the Twig integration",
        "kriswallsmith/spork": "to be able to dump assets in parallel"
    },
    "autoload": {
        "psr-4": { "Symfony\\Bundle\\AsseticBundle\\": "" }
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.8-dev"
        }
    }
}
